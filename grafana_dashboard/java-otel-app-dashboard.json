  "id": null,
  "uid": "java-otel-app-dashboard",
  "title": "Java OTEL Sample App Monitoring",
  "tags": ["java", "otel", "microservice"],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "reqps",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
      "id": 1,
      "title": "Request Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(http_requests_total{app=\"java-otel-sample-app\"}[5m])",
          "legendFormat": "Requests/sec"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 10}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0},
      "id": 2,
      "title": "Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "vector(0)",
          "legendFormat": "Error %"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0},
      "id": 3,
      "title": "CPU Usage",
      "type": "stat",
      "targets": [
        {
          "expr": "java_cpu_usage_percent{app=\"java-otel-sample-app\"}",
          "legendFormat": "CPU %"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
      "id": 4,
      "title": "Available Replicas",
      "type": "stat",
      "targets": [
        {
          "expr": "count(up{app=\"java-otel-sample-app\"})",
          "legendFormat": "Replicas"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 5,
      "title": "Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_sum{app=\"java-otel-sample-app\"}[5m]) / rate(http_server_requests_seconds_count{app=\"java-otel-sample-app\"}[5m])",
          "legendFormat": "Avg Response Time"
        }
      ]
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "bytes",
          "min": 0
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 6,
      "title": "Memory Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{app=\"java-otel-sample-app\",area=\"heap\"}",
          "legendFormat": "Heap Memory"
        }
      ]
    },
    {
      "datasource": {
        "type": "cloudwatch",
        "uid": "${DS_CLOUDWATCH}"
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
      "id": 7,
      "title": "Application Logs",
      "type": "logs",
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      },
      "targets": [
        {
          "datasource": {
            "type": "cloudwatch",
            "uid": "${DS_CLOUDWATCH}"
          },
          "expression": "fields @timestamp, @message | filter @message like /java-otel-sample-app/ | sort @timestamp desc | limit 20",
          "logGroups": [
            {
              "name": "/aws/eks/fargate-platform/applications",
              "arn": "arn:aws:logs:region:account:/aws/eks/fargate-platform/applications"
            }
          ],
          "queryMode": "Logs",
          "refId": "A",
          "region": "default"
        }
      ]
    }
  ],
